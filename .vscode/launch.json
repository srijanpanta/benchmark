{
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "lambda-python3.12:HelloWorldFunction (python3.12)",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/Aws Project/lambda-python3.12/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {},
                "runtime": "python3.12"
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "API lambda-python3.12:HelloWorldFunction (python3.12)",
            "invokeTarget": {
                "target": "api",
                "templatePath": "${workspaceFolder}/Aws Project/lambda-python3.12/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "api": {
                "path": "/hello",
                "httpMethod": "get",
                "payload": {
                    "json": {}
                }
            },
            "lambda": {
                "runtime": "python3.12"
            }
        },
        {
            "name": "Attach to Python Functions",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 9091
            },
            "preLaunchTask": "func: host start"
        }
    ]
}